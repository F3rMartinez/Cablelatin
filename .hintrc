{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/forms": [
      "default",
      {
        "select-name": "off",
        "label": "off"
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "user-select"
        ]
      }
    ],
    "meta-charset-utf-8": "off",
    "meta-viewport": "off",
    "axe/aria": [
      "default",
      {
        "aria-allowed-attr": "off"
      }
    ]
  }
}